Package: isisinsight
Title: ISIS-Fish insight R package
Version: 0.0.0.9000
Authors@R: c(
    person("Mathieu", "Depetris", , "mathieu.depetris@cnrs.fr", role = c("aut", "cre"),
           comment = c(ORCID = "0000-0001-8080-0531")),
    person("Stephanie", "Mahevas", , "stephanie.mahevas@ifremer.fr", role = c("aut")),
